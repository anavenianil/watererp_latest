<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle"/>

    <!--
        Added the entity ApplicationTxn.
    -->
    <changeSet id="20160229075018" author="jhipster">
        <createTable tableName="application_txn">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="s_house_no" type="varchar(255)"/>
            
            <column name="govt_official_no" type="varchar(255)"/>
            
            <column name="ward" type="varchar(255)"/>
            
            <column name="street" type="varchar(255)"/>
            
            <column name="pincode" type="varchar(255)"/>
            
            <column name="block" type="varchar(255)"/>
            
            <column name="area" type="varchar(255)"/>
            
            <column name="section" type="varchar(255)"/>
            
            <column name="constituency" type="varchar(255)"/>
            
            <column name="email" type="varchar(255)"/>
            
            <column name="telephone_number" type="varchar(255)"/>
            
            <column name="mobile" type="varchar(255)"/>
            
            <column name="scan_plan" type="${floatType}"/>
            
            <column name="scan_plan1" type="${floatType}"/>
            
            <column name="sale_deed" type="${floatType}"/>
            
            <column name="sale_deed1" type="${floatType}"/>
            
            <column name="total_plinth_area" type="${floatType}"/>
            
            <column name="created_date" type="timestamp"/>
            
            <column name="updated_date" type="timestamp"/>
            
            <column name="status" type="varchar(255)"/>
            
            <column name="application_type_master_id" type="bigint"/>
            <column name="connection_type_master_id" type="bigint"/>
            <column name="ctegory_master_id" type="bigint"/>
            <column name="pipe_size_master_id" type="bigint"/>
            <column name="sewer_size_id" type="bigint"/>
            <column name="file_number_id" type="bigint"/>
            <column name="customer_id" type="bigint"/>
            <!-- jhipster-needle-liquibase-add-column - Jhipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="application_txn" columnName="created_date" columnDataType="datetime"/>

        <dropDefaultValue tableName="application_txn" columnName="updated_date" columnDataType="datetime"/>

        
        <addForeignKeyConstraint baseColumnNames="application_type_master_id"
                                 baseTableName="application_txn"
                                 constraintName="fk_applicationtxn_applicationtypemaster_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application_type_master"/>

        <addForeignKeyConstraint baseColumnNames="connection_type_master_id"
                                 baseTableName="application_txn"
                                 constraintName="fk_applicationtxn_connectiontypemaster_id"
                                 referencedColumnNames="id"
                                 referencedTableName="connection_type_master"/>

        <addForeignKeyConstraint baseColumnNames="ctegory_master_id"
                                 baseTableName="application_txn"
                                 constraintName="fk_applicationtxn_ctegorymaster_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ctegory_master"/>

        <addForeignKeyConstraint baseColumnNames="pipe_size_master_id"
                                 baseTableName="application_txn"
                                 constraintName="fk_applicationtxn_pipesizemaster_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pipe_size_master"/>

        <addForeignKeyConstraint baseColumnNames="sewer_size_id"
                                 baseTableName="application_txn"
                                 constraintName="fk_applicationtxn_sewersize_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sewer_size"/>

        <addForeignKeyConstraint baseColumnNames="file_number_id"
                                 baseTableName="application_txn"
                                 constraintName="fk_applicationtxn_filenumber_id"
                                 referencedColumnNames="id"
                                 referencedTableName="file_number"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="application_txn"
                                 constraintName="fk_applicationtxn_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

    </changeSet>
</databaseChangeLog>
